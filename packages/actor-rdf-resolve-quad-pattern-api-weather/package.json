{
  "name": "example-actor-rdf-resolve-quad-pattern-api-weather",
  "version": "1.0.0",
  "description": "An example illustrating how to enable querying over a custom API",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "my-comunica": "./bin/query.js",
    "my-comunica-http": "./bin/http.js",
    "my-comunica-dynamic": "./bin/query-dynamic.js"
  },
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/example-actor-rdf-resolve-quad-pattern-api-weather",
  "lsd:components": "components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/example-actor-rdf-resolve-quad-pattern-api-weather/^1.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/example-actor-rdf-resolve-quad-pattern-api-weather/^1.0.0/components/": "components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/example-actor-rdf-resolve-quad-pattern-api-weather/^1.0.0/config/": "config/"
  },
  "files": [
    "components",
    "config",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.js",
    "index.d.ts",
    "index-browser.d.ts",
    "index-browser.js",
    "engine-default.js"
  ],
  "dependencies": {
    "@comunica/actor-init-sparql": "^1.14.0",
    "@comunica/core": "^1.14.0",
    "@comunica/bus-rdf-resolve-quad-pattern": "^1.14.0",
    "@comunica/runner": "^1.14.0",
    "@rdfjs/data-model": "^1.1.2",
    "@types/minimist": "^1.2.0",
    "@types/n3": "^1.1.5",
    "asynciterator": "^3.0.1"
  },
  "devDependencies": {
    "typescript": "^3.8.3"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build:engine": "comunica-compile-config config/config-default.json > engine-default.js",
    "build:lib": "tsc",
    "build": "npm run build:lib && npm run build:engine",
    "build-watch": "tsc --watch",
    "postinstall": "npm run build"
  },
  "browser": {
    "./index.js": "./index-browser.js"
  }
}
